<?xml version='1.0' encoding='utf-8'?>
<widget id="com.debnath.habitchart" version="4.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HabitChart</name>
    <description>
        keep track of your habits
    </description>
    <author email="jayanta.dn@gmail.com" href="https://github.com/jayantadn/HabitChart">
        Jayanta Debnath
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/img/logo.png" />
</widget>
