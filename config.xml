<?xml version='1.0' encoding='utf-8'?>
<widget id="com.atwebpages.debnath.habitjournal" version="0.6.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name short="HabitJournal">Habit Journal</name>
    <description>
        Habit tracking app.
    </description>
    <author email="jayanta.dn@gmail.com" href="http://debnath.atwebpages.com/">
        Jayanta Debnath
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <icon src="www/img/logo.png" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
